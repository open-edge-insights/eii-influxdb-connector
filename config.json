{
    "config": {
        "influxdb": {
            "retention": "1h30m5s",
            "username": "admin",
            "password": "admin123",
            "dbname": "datain",
            "ssl": "True",
            "verifySsl": "False",
            "port": "8086"
        },
        "pub_workers": "5",
        "sub_workers": "5",
        "ignore_keys": [ "defects" ],
        "tag_keys": [],
        "blacklist_query": ["CREATE","DROP","DELETE","ALTER","<script>"]
    },
    "interfaces": {
        "Servers": [
            {
                "Name": "InfluxDBConnector",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:65145",
                "AllowedClients": [
                    "Visualizer","DiscoveryCreek","OpcuaExport","RestDataExport"
                ]
            }
        ],
        "Publishers": [
            {
                "Name": "PointData",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:65015",
                "Topics": [
                    "point_data"
                ],
                "AllowedClients": [
                    "*"
                ]
            },
            {
                "Name": "PointClassifierResults",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:65016",
                "Topics": [
                    "point_classifier_results"
                ],
                "AllowedClients": [
                    "*"
                ]
            },
            {
                "Name": "HumidityClassifierResults",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:65030",
                "Topics": [
                    "humidity_classifier_results"
                ],
                "AllowedClients": [
                    "*"
                ]
            },
            {
                "Name": "TSData",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:65031",
                "Topics": [
                    "ts_data"
                ],
                "AllowedClients": [
                    "*"
                ]
            },
            {
                "Name": "RFCResults",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:65032",
                "Topics": [
                    "rfc_results"
                ],
                "AllowedClients": [
                    "*"
                ]
            }
        ],
        "Subscribers": [
            {
                "Name": "video",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:65013",
                "PublisherAppName": "VideoAnalytics",
                "Topics": [
                    "camera1_stream_results"
                ]
            },
            {
                "Name": "dc",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:65014",
                "PublisherAppName": "DiscoveryCreek",
                "Topics": [
                    "dc_point_data_results"
                ]
            }            
        ]
        }
}
