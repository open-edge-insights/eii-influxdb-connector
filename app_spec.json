{
    "Module" : {
        "Name": "InfluxDBConnector",
        "Description": "InfluxDBConnector module manifest",
        "SchemaVersion": "0.2",
        "ManifestFile": "${idx:influxdbconnector:$EIS_VERSION}",
        "RunAsUser": "$EIS_UID",
        "RunAsGroup": "$EIS_UID",
        "Resources": {
            "CPU": 100,
            "MemoryMB": 100
        },
        "ExecutionEnv": {
            "AppName": "InfluxDBConnector",
            "ETCD_ENDPOINT": "${datastore.endpoint}",
            "CONFIGMGR_CACERT": "${databucket.cacert}",
            "CONFIGMGR_CERT": "${databucket.cert}",
            "CONFIGMGR_KEY": "${databucket.key}",
            "ETCD_PREFIX": "$ETCD_PREFIX",
            "DEV_MODE": "$DEV_MODE",
            "PROFILING_MODE": "false",
            "ZMQ_RECV_HWM": "50",

            "SERVER_InfluxDBConnector_ENDPOINT": "0.0.0.0:${ep.influxEISServer.localport}",
            "PUBLISHER_PointData_ENDPOINT" : "0.0.0.0:${ep.pointDataOutput.localport}",
            "PUBLISHER_PointData_TYPE" : "zmq_tcp",
            "PUBLISHER_PointClassifierResults_ENDPOINT" : "0.0.0.0:${ep.pointClsOutput.localport}",
            "PUBLISHER_PointClassifierResults_TYPE" : "zmq_tcp",
            "PUBLISHER_RFCResults_ENDPOINT" : "0.0.0.0:${ep.rfcOutput.localport}",
            "PUBLISHER_RFCResults_TYPE" : "zmq_tcp",
            "PUBLISHER_TSData_ENDPOINT" : "0.0.0.0:${ep.tsdataOutput.localport}",
            "PUBLISHER_TSData_TYPE" : "zmq_tcp",
            "PUBLISHER_HumidityClassifierResults_ENDPOINT" : "0.0.0.0:${ep.humdclsOutput.localport}",
            "PUBLISHER_HumidityClassifierResults_TYPE" : "zmq_tcp",
            "SUBSCRIBER_video_ENDPOINT" : "${ep.influxSub.remoteaddress}:${ep.influxSub.remoteport}",
            "SUBSCRIBER_video_TYPE" : "zmq_tcp",
            "SUBSCRIBER_dc_ENDPOINT" : "${ep.influxDcSub.remoteaddress}:${ep.influxDcSub.remoteport}",
            "SUBSCRIBER_dc_TYPE" : "zmq_tcp"
        },
        "Endpoints": [
            {
                "Name": "influxEISServer",
                "Endtype": "server",
                "Port": "8675",
                "DataType": "messages",
                "Link": "sinfluxout"
            },
            {
                "Name": "influxSub",
                "Endtype": "client",
                "DataType": "messages"
            },
            {
                "Name": "influxServer",
                "Endtype": "server",
                "Port": "8086",
                "DataType": "messages",
                "Link": "influxout"
            },
            {
                "Name": "pointDataOutput",
                "Endtype": "server",
                "Port": "65015",
                "DataType": "messages",
                "Link": "pdinfluxout"
            },
            {
                "Name": "pointClsOutput",
                "Endtype": "server",
                "Port": "65016",
                "DataType": "messages",
                "Link": "pcinfluxout"
            },
            {
                "Name": "rfcOutput",
                "Endtype": "server",
                "Port": "65032",
                "DataType": "messages",
                "Link": "rfcinfluxout"
            },
            {
                "Name": "tsdataOutput",
                "Endtype": "server",
                "Port": "65031",
                "DataType": "messages",
                "Link": "tsinfluxout"
            },
            {
                "Name": "humdclsOutput",
                "Endtype": "server",
                "Port": "65030",
                "DataType": "messages",
                "Link": "humdinfluxout"
            },
            {
                "Name": "influxDcSub",
                "Endtype": "client",
                "DataType": "messages",
                "Link": "dc-output"
            }
        ]
    }
}
